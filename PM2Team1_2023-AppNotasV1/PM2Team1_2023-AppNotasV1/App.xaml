             x:Class="PM2Team1_2023_AppNotasV1.App">
    <!--
        Define global resources and styles here, that apply to all pages in your app.
    -->
    <Application.Resources>
        <ResourceDictionary>
            <OnPlatform x:TypeArguments="x:String" x:Key="FontAwesomeRegular">
               <On Platform="Android" Value="FontAwesome6Brands-Regular-400.otf#Regular">
                </On>
            </OnPlatform>
        </ResourceDictionary>        
    </Application.Resources>
</Application>
